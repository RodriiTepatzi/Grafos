        mc:Ignorable="d"
        Title="Expandir" Height="350" Width="650" Foreground="Black" Background="White" ResizeMode="NoResize" WindowStartupLocation="CenterScreen" WindowStyle="ToolWindow" PreviewKeyUp="Window_PreviewKeyUp">
